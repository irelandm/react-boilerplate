{
  "presets": [
    ["@babel/preset-env", {
    "targets": [
      "last 2 versions",
      "not dead",
      "not IE 11"
    ],
    "corejs": 3,
    "useBuiltIns": "entry"
  }], "@babel/preset-react"],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import"
  ],
  "env": {
    "test": {
      "plugins": ["dynamic-import-node"]
    }
  }
}